{
  "name": "Member",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "_base": "User",
  "mysql": {
    "schema": "brainfock_node",
    "table": "user"
  },
  "hidden": [
    "password"
  ],
  "properties": {
    "realm": {
      "type": "string"
    },
    "username": {
      "type": "string"
    },
    "password": {
      "type": "string",
      "required": true
    },
    "credentials": {
      "type": "object",
      "deprecated": true
    },
    "challenges": {
      "type": "object",
      "deprecated": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "emailVerified": {
      "type": "boolean"
    },
    "verificationToken": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "created": {
      "type": "date"
    },
    "lastUpdated": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "roleMappings": {
      "type": "hasMany",
      "model": "RoleMapping",
      "foreignKey": "principalId",
      "scope": {
        "where": {
          "principalType": "USER"
        }
      }
    },
    "roles": {
      "type": "hasMany",
      "model": "Role",
      "foreignKey": "principalId",
      "through": "RoleMapping"
    }
  },
  "acls": [],
  "methods": {}
}
